{
  "name": "test-lab",
  "version": "1.0.0",
  "description": "Test-Lab",
  "main": "index.tsx",
  "scripts": {
    "deploy": "gh-pages -d dist -e test-lab",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "precommit": "npm run lint",
    "pret": "prettier . --write --ignore-unknown",
    "prepare": "husky install",
    "watch": "webpack --watch",
    "start": "webpack serve",
    "commit": "git add . && git cz",
    "commitlint": "commitlint --edit",
    "commitizen": "exec < /dev/tty && git cz --hook || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milabozhevskaya/test-lab.git"
  },
  "author": "milabozhevskaya",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/milabozhevskaya/test-lab/issues"
  },
  "homepage": "https://github.com/milabozhevskaya/test-lab#readme",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "@reduxjs/toolkit": "^2.2.5",
    "normalize.scss": "^0.1.0",
    "ps-node": "^0.1.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/react-redux": "^7.1.33",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@svgr/webpack": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "commitizen": "^4.3.0",
    "css-loader": "^6.7.3",
    "cz-conventional-changelog": "^3.3.0",
    "esbuild-loader": "^2.20.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "gh-pages": "^5.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^9.0.7",
    "mini-css-extract-plugin": "^2.7.2",
    "nodemon-webpack-plugin": "^4.8.1",
    "prettier": "^3.2.4",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2",
    "webpack-dev-server": "^4.13.3"
  },
  "type": "module",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "^5.59.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
